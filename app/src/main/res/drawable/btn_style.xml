            android:shape="rectangle">
            <corners android:radius="8dp"/>
            <!--<stroke android:width="2dp"-->
                <!--android:color="#000000"/>-->
            <gradient android:startColor="#53a619"
                android:endColor="#558b2f"/>

        </shape>
    </item>
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="rectangle">
            <corners android:radius="8dp"/>
        <gradient android:startColor="#7cb342"
            android:endColor="#2b460f"
            android:angle="270"/>
        </shape>


    </item>
</selector>